{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "prettier"
  ],
  "plugins": ["vue"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-console": "warn",
    "no-debugger": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  }
}